									EquipWeapon(_G.Select_Weapon)
									StatrMagnetBoneMon = true
									v.HumanoidRootPart.CanCollide = false
									v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
									PosMonMysticDroplet = v.HumanoidRootPart.CFrame
									Tween(v.HumanoidRootPart.CFrame * F)
									FastA()
								until getgenv().MysticDroplet == false or not v.Parent or v.Humanoid.Health <= 0
							end
						end
					end
				else
					StatrMagnetBoneMon = false
					if (CFrame.new(-3250.3955078125, 298.6902770996094, -10550.8759765625).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1500 then
						Tween(CFrame.new(-3250.3955078125, 298.6902770996094, -10550.8759765625))
					else
						TelePBoss(CFrame.new(-3250.3955078125, 298.6902770996094, -10550.8759765625))
					end
				end
			end)
		end
	end
end)

