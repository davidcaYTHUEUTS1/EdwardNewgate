		    MagmaOrePOS = CFrame.new(-5653.779296875, 57.52100372314453, -5650.1357421875)
        elseif TypeMagmaOre == 2 then
			MagmaOrePOS = CFrame.new(-5334.0361328125, 41.74039840698242, -4738.40283203125)
		end
        end
	end)

