				for i,v in pairs(game:GetService("Workspace")["_WorldOrigin"].Locations:GetChildren()) do
					if v.Name == "CurvedRing" or v.Name == "SlashHit" or v.Name == "SwordSlash" or v.Name == "SlashTail" then
